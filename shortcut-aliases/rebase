[alias]
  rb = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbdate = rebase --committer-date-is-author-date
  rbet = rebase --edit-todo
  rbh = rebase -h
  rbi = rebase --interactive
  rbq = rebase --quiet
  rbis = rebase --interactive --autostash
  rbs = rebase --autostash
  rbsq = rebase --interactive --autosquash
  rbsqs = rebase --interactive --autosquash --autostash
  rbv = rebase --verbose
  