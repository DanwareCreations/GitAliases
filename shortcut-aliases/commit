[alias]
  cm = commit
  cma = commit --all
  cmh = commit -h
  cmi = commit --include
  cmm = commit --message
  cmam = commit --all --message
  cmsq = commit --no-edit --squash
  cmasq = commit --all --no-edit --squash
  cmo = commit --only
  cmp = commit --patch
  cmq = commit --quiet
  cmv = commit --verbose
  