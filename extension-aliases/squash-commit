[alias]
  xsq = !"f() { commit=$(git rev-parse $1); git commit --no-edit --squash $commit; git rebase --interactive --autostash --autosquash $commit^; }; f"
  xasq = !"f() { commit=$(git rev-parse $1); git commit --all --no-edit --squash $commit; git rebase --interactive --autostash --autosquash $commit^; }; f"